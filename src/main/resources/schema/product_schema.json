{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Root Schema",
    "required": [
        "productId",
        "productName",
        "productPrice",
        "productDescription"
    ],
    "properties": {
        "productId": {
            "type": "integer",
            "title": "The productId Schema",
            "examples": [
                1
            ]
        },
        "productName": {
            "type": "string",
            "title": "The productName Schema",
            "examples": [
                "Scooter"
            ]
        },
        "productPrice": {
            "type": "number",
            "title": "The productPrice Schema",
            "examples": [
                599.99
            ]
        },
        "productDescription": {
            "type": "string",
            "title": "The productDescription Schema",
            "examples": [
                "Only for me"
            ]
        }
    }
}